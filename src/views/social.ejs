<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>SSO Login Social</title>
    <!-- Latest compiled and minified CSS -->    
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script>
      $(document).ready(function(){
        const ssoToken = $('#ssoToken').val();
        const redirectURL = `${localStorage.getItem("clientURL")}/code?ssoToken=${ssoToken}`;
        localStorage.removeItem("clientURL");
        window.location.href = redirectURL;

      });
      </script>
  </head>

  <body>
    <input hidden value="<%=ssoToken %>" hidden id="ssoToken"/>
  </body>
</html>
